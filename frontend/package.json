{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build --sourcemap true",
        "preview": "vite preview",
        "format": "prettier --config .prettierrc.json --write src/",
        "gen-api": "bash scripts/gen-api.sh",
        "ci": "tsc && vite build --sourcemap true && prettier --check src/ && eslint 'src/**/*.{ts,tsx}' --report-unused-disable-directives --max-warnings 0 && missing=$(grep -Er \"\\\"(\\w|-)+\\.(\\w|-)+\\\"\" public/locales/); count=$(echo -n \"$missing\" | grep -c '^'); [ \"$count\" -gt 0 ] && echo \"$missing\" && echo \"$count untranslated entries found\" && exit 1 || exit 0"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.3",
        "@heroicons/react": "^2.2.0",
        "@tailwindcss/vite": "^4.0.7",
        "@tanstack/react-form": "^1.1.2",
        "@tanstack/react-router": "^1.112.0",
        "@tanstack/router-plugin": "^1.112.0",
        "clsx": "^2.1.1",
        "framer-motion": "^12.5.0",
        "i18next-browser-languagedetector": "^8.0.4",
        "i18next-http-backend": "^3.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-i18next": "^15.4.1",
        "react-toastify": "^11.0.5"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.18.4",
        "@tanstack/router-devtools": "^1.40.0",
        "@tanstack/router-plugin": "^1.40.3",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.57.0",
        "eslint-plugin-jsdoc": "^48.2.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.13",
        "tailwindcss": "^4.0.7",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.5.0",
        "vite": "^5.4.17",
        "vite-plugin-static-copy": "^2.2.0",
        "vite-plugin-svgr": "^4.2.0"
    }
}
